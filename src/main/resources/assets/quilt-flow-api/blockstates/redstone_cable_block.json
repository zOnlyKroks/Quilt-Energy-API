{
  "multipart": [
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_base"
      },
      "when": {
        "show_base": true,
        "powered": false
      }
    },
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_connection"
      },
      "when": {
        "connection_up": true,
        "powered": false
      }
    },
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_connection",
        "x": 180
      },
      "when": {
        "connection_down": true,
        "powered": false
      }
    },
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_connection",
        "x": 90
      },
      "when": {
        "connection_north": true,
        "powered": false
      }
    },
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_connection",
        "x": 270
      },
      "when": {
        "connection_south": true,
        "powered": false
      }
    },
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_connection",
        "x": 90,
        "y": 90
      },
      "when": {
        "connection_east": true,
        "powered": false
      }
    },
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_connection",
        "x": 270,
        "y": 90
      },
      "when": {
        "connection_west": true,
        "powered": false
      }
    },
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_base_on"
      },
      "when": {
        "show_base": true,
        "powered": true
      }
    },
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_connection_on"
      },
      "when": {
        "connection_up": true,
        "powered": true
      }
    },
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_connection_on",
        "x": 180
      },
      "when": {
        "connection_down": true,
        "powered": true
      }
    },
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_connection_on",
        "x": 90
      },
      "when": {
        "connection_north": true,
        "powered": true
      }
    },
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_connection_on",
        "x": 270
      },
      "when": {
        "connection_south": true,
        "powered": true
      }
    },
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_connection_on",
        "x": 90,
        "y": 90
      },
      "when": {
        "connection_east": true,
        "powered": true
      }
    },
    {
      "apply": {
        "model": "quilt-flow-api:block/redstone_cable_block_connection_on",
        "x": 270,
        "y": 90
      },
      "when": {
        "connection_west": true,
        "powered": true
      }
    }
  ]
}
